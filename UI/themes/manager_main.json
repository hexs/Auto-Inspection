{
  "label": {
    "colours": {
      "normal_text": "#000000",
      "disabled_text": "",
      "text_shadow": "#ffffff"
    },
    "font": {
      "name": "montserrat",
      "size": "12",
      "bold": "0",
      "italic": "0"
    },
    "misc": {
      "text_horiz_alignment": "left",
      "text_shadow": "1",
      "text_shadow_size": "1",
      "text_shadow_offset": "0,0"
    }
  },
  "@minimize": {
    "images": {
      "normal_image": {
        "path": "UI\\main button w\\minimize.png",
        "sub_surface_rect": "0,0,45,30"
      },
      "hovered_image": {
        "path": "UI\\main button w\\minimize.png",
        "sub_surface_rect": "0,30,45,30"
      }
    }
  },
  "@maximize": {
    "images": {
      "normal_image": {
        "path": "UI\\main button w\\maximize.png",
        "sub_surface_rect": "0,0,45,30"
      },
      "hovered_image": {
        "path": "UI\\main button w\\maximize.png",
        "sub_surface_rect": "0,30,45,30"
      }
    }
  },
  "@close": {
    "images": {
      "normal_image": {
        "path": "UI\\main button w\\close.png",
        "sub_surface_rect": "0,0,45,30"
      },
      "hovered_image": {
        "path": "UI\\main button w\\close.png",
        "sub_surface_rect": "0,30,45,30"
      }
    }
  },
  "horizontal_slider": {
    "colours": {
      "normal_bg": "#FF0000",
      "hovered_bg": "#35393e",
      "disabled_bg": "#25292e",
      "selected_bg": "#25292e",
      "active_bg": "#193784",
      "dark_bg": "#84878A",
      "normal_text": "#c5cbd8",
      "hovered_text": "#FFFFFF",
      "selected_text": "#FFFFFF",
      "disabled_text": "#6d736f"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "10",
      "enable_arrow_buttons": "1",
      "sliding_button_width": "30"
    }
  },
  "horizontal_slider.button": {
    "misc": {
      "border_width": "1"
    }
  },
  "horizontal_slider.#sliding_button": {
    "colours": {
      "normal_bg": "#FF0000"
    }
  },
  "horizontal_slider.#left_button": {
    "images": {
      "normal_image": {
        "path": "UI\\main button w\\zoom.png",
        "sub_surface_rect": "0,20,30,20"
      },
      "hovered_image": {
        "path": "UI\\main button w\\zoom.png",
        "sub_surface_rect": "0,0,30,20"
      }
    }
  },
  "horizontal_slider.#right_button": {
    "images": {
      "normal_image": {
        "path": "UI\\main button w\\zoom.png",
        "sub_surface_rect": "30,20,30,20"
      },
      "hovered_image": {
        "path": "UI\\main button w\\zoom.png",
        "sub_surface_rect": "30,0,30,20"
      }
    }
  }
}
